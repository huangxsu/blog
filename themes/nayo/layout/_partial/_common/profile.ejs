<% var profile = theme.profile %>

<div class="profile sildeUpMin">

		
		<img class="avatar" src="<%- url_for(profile.avatar) %>">
		<p class="author"><%= profile.author || config.author %></p>
		
		<div class="social">
   		<% for (var i in theme.social) { %>      		
		<a target="_blank" class="social-links" href="<%- (theme.social[i]) %>" >		
			<i class="iconfont icon-<%= i %>" ></i>
		</a>	
	    <% } %>
    	</div>		
</div>